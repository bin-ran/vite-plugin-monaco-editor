{
  "name": "vite-plugin-monaco-editor",
  "type": "module",
  "version": "0.0.1",
  "description": "A Vite plugin that simplifies the integration of Monaco Editor with automatic worker handling and language support",
  "author": "bin-ran",
  "keywords": [
    "vite",
    "plugin",
    "monaco-editor",
    "editor",
    "code-editor",
    "typescript",
    "javascript",
    "web-worker"
  ],
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "example:dev": "pnpm build && vite --config example/vite.config.ts",
    "example:build": "pnpm build && vite build --config example/vite.config.ts",
    "example:preview": "vite preview --config example/vite.config.ts"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@eslint/js": "^9.39.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "tsdown": "^0.20.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "peerDependencies": {
    "monaco-editor": "^0.55.1",
    "vite": "^7.3.1"
  }
}
